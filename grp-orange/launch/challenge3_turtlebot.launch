<?xml version="1.0"?>
<launch>
    	
    <include file="$(find turtlebot_bringup)/launch/minimal.launch" />
    <node type="urg_node" name="scanner" pkg="urg_node" />  
    <include file="$(find realsense2_camera)/launch/rs_aligned_depth.launch"/>
    <node name="basetf" pkg="tf" type="static_transform_publisher" args=" 0,1 0 0 0 0 0 base_footprint laser 100"/>
	<group>
        <node pkg="gmapping" type="slam_gmapping" name="gmapping_node" >
            <param name="maxUrange" value="4.0"/>  
        </node>      
    </group>
    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find grp-orange)/rviz_config/rviz_laser_config.rviz" />
    <node type="main_real.py" name="control" pkg="grp-orange"/>
	<node type="detection.py" name="detection" pkg="grp-orange" output="screen"/>
    <node type="trace_robot.py" name="traceRobot" pkg="grp-orange"/>
	
</launch>
