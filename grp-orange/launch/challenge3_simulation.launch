<?xml version="1.0"?>
<launch>

	<!-- <env name="GAZEBO_RESOURCE_PATH" value="$(env HOME)/.gazebo" /> -->
	<!-- Note: the world_name is with respect to GAZEBO_RESOURCE_PATH environmental variable -->
	<include file="$(find tbot_gazebo)/launch/start_world.launch">
		<arg name="world_file" value="$(find larm)/models/challenge-1.world" />
	</include>

    <include file="$(find tbot_gazebo)/launch/spawn_tbot.launch">
	    <arg name="x" value="-3.32" />
	    <arg name="y" value="-2.95" />
	    <arg name="yaw" value="2.86" />
    </include>

	<node pkg="gmapping" type="slam_gmapping" name="gmapping_node" >
            
    </node>
    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find grp-orange)/rviz_config/rviz_laser_config.rviz" />
    <node type="main.py" name="control" pkg="grp-orange" output="screen"/>	
	<node type="trace_robot.py" name="traceRobot" pkg="grp-orange"/>
</launch>